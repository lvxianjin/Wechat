<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Wechat</display-name>
  <servlet>
    <servlet-name>ListServlet</servlet-name>
    <servlet-class>com.qdlg.servlet.ListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListServlet</servlet-name>
    <url-pattern>/list.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteOneServlet</servlet-name>
    <servlet-class>com.qdlg.servlet.DeleteOneServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteOneServlet</servlet-name>
    <url-pattern>/deleteone.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteBatchServlet</servlet-name>
    <servlet-class>com.qdlg.servlet.DeleteBatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteBatchServlet</servlet-name>
    <url-pattern>/DeleteBatchServlet.action</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>InitTalkServlet</servlet-name>
  	<servlet-class>com.qdlg.servlet.InitTalkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InitTalkServlet</servlet-name>
  	<url-pattern>/InitTalkServlet.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AutoReplyServlet</servlet-name>
    <servlet-class>com.qdlg.servlet.AutoReplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AutoReplyServlet</servlet-name>
    <url-pattern>/AutoReplyServlet.action</url-pattern>
  </servlet-mapping>
  
</web-app>